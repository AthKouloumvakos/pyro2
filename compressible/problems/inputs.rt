# simple inputs files for the four-corner problem.

[driver]
max_steps = 1000
tmax = 100.0


[io]
basename = rt_
n_out = 100


[mesh]
nx = 128
ny = 384
xmax = 1.0
ymax = 3.0

xlboundary = periodic
xrboundary = periodic

ylboundary = reflect
yrboundary = reflect


[rt]



[compressible]
grav = -1.0

limiter = 2
