[driver]
max_steps = 2000
tmax = 4.0


[compressible]
limiter = 2
cvisc = 0.1


[io]
basename = advect_
dt_out = 0.5


[eos]
gamma = 1.4


[mesh]
nx = 64
ny = 64
xmax = 1.0
ymax = 1.0

xlboundary = periodic
xrboundary = periodic

ylboundary = periodic
yrboundary = periodic


[advect]
r_init = 0.2


[vis]
dovis = 0
